{
    "name": "huang-yi/laravel-rbac",
    "description": "Role-Based Access Control for Laravel.",
    "keywords": ["rbac", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Huang Yi",
            "email": "coodeer@163.com"
        }
    ],
    "require": {
        "illuminate/config": "~5.5",
        "illuminate/database": "~5.5",
        "illuminate/routing": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "illuminate/cache": "~5.5",
        "illuminate/view": "~5.5",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "HuangYi\\Rbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HuangYi\\Rbac\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HuangYi\\Rbac\\RbacServiceProvider"
            ]
        }
    }
}
